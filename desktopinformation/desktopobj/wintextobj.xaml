<src:WinObjBase x:Class="DesktopInformation.DesktopObj.WinTextObj"
        xmlns:src="clr-namespace:DesktopInformation.DesktopObj"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopInformation.DesktopObj"
        xmlns:uc="clr-namespace:DesktopInformation.UserControls"
        mc:Ignorable="d" >

    <Viewbox>
        <Grid x:Name="grd">
            <TextBlock x:Name="tbk" Background="Transparent"  Foreground="White"/>
            <uc:RradualChangedTextBlock x:Name="tbkAni" Background="Transparent" BorderThickness="0" Foreground="White"/>
        </Grid>
    </Viewbox>

</src:WinObjBase>
