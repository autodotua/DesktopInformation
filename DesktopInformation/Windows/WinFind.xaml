<Window x:Class="DesktopInformation.Windows.WinFind"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:picker="clr-namespace:FzLib.Control.Picker;assembly=FzWpfControlLib"
   mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="WinFind" SizeToContent="WidthAndHeight">
    <Grid Margin="4">
        <TabControl>
            <TabItem Header="查询Tick">
                <StackPanel>
                    <picker:DatePicker SelectedDateChanged="DatePicker_SelectedDateChanged"/>
                    <TextBlock FontSize="5"/>
                    <TextBox BorderThickness="0" x:Name="txtTick"/>
                </StackPanel>
             
            </TabItem>
        </TabControl>
    </Grid>
</Window>
